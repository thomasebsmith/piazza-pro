{
  "manifest_version": 2,
  "name": "Piazza Pro",
  "author": "Thomas Smith",
  "version": "0.2",
  "description": "Adds many useful features to Piazza",
  "homepage_url": "https://github.com/thomasebsmith/piazza-pro",
  "content_scripts": [
    {
      "matches": ["https://piazza.com/*"],
      "js": ["content.js"]
    },
    {
      "matches": ["https://piazza.com/*"],
      "css": ["darkMode.css"],
      "run_at": "document_start"
    }
  ]
}
